<%= simple_form_for @pictures, :html => { :multipart => true } do |f| %>
	<%= select_tag(:location_id, options_for_select(@locations), :prompt => "Select a location") %>
	<%= f.file_field :book_img %>
	<%= f.input :title, label: "Book Title" %>
	<%= f.input :url, label: "DE" %>
	<%= f.button :submit, :class => 'btn-custom2' %>
<% end %>
